<%if current_user%>

<div class= "container">
<div class= "row">
<div class= "col-md-6">

<%=form_with model:@center,local: true do |f|%>
            <div class= "form-group">

        <%=f.label :name%>
        <%=f.text_field :name, class: "form-control"%>
        </div>


            <div class= "form-group">

        <%=f.label :area%>
        <%=f.text_field :area, class: "form-control"%>
        </div>


            <div class= "form-group">

        <%=f.label :city%>
        <%=f.text_field :city, class: "form-control"%>
        </div>


            <div class= "form-group">

        <%=f.label :state%>
        <%=f.text_field :state, class: "form-control"%>
        </div>


            <div class= "form-group">

        <%=f.label :pincode%>
        <%=f.text_field :pincode,  class: "form-control"%>
        </div>

    
            <div class= "form-group">

        <%=f.label :email%>
        <%=f.text_field :email, class: "form-control"%>
        </div>

    <%=f.submit  class: "btn btn-primary"%>
<%end%>

   </div>
   </div>
   </div>
   <%else%>
<h3>Please login!!!!!!!!</h3>
<h3>If you are new user then sign up</h3>
<%= link_to 'sign in', new_user_registration_path %>
<%= link_to 'sign in', new_user_session_path %>
<%end%>
