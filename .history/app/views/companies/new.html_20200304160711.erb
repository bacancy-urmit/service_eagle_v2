<%if user_signed_in?%>
<div class= "container">
<div class= "row">
<div class= "col-md-6"> 
<%=form_with model:@company,url: companies_path,local: true do |c| %>
    <p>
        <%=c.label :company_name%>
        <%=c.text_field :name ,placeholder: "eg.Bajaj"%>
    <p>

    <p>
        <%=c.label :website%>
        <%=c.text_field :website ,placeholder: "eg.www.bajaj.com"%>
    <p>

    <p>
        <%=c.label :email%>
        <%=c.text_field :email ,placeholder: "eg.Bajaj@gmail.com"%>
    <p>
    <%=c.submit %>
<%end%>
<%= link_to 'add admin', new_super_admin_path%>
</div>
</div>
</div>
<%else%>
<h3>Please login!!!!!!!!</h3>
<h3>If you are new user then sign up</h3>
<%= link_to 'sign in', new_user_registration_path %>
<%= link_to 'sign in', new_user_session_path %>
<%end%>