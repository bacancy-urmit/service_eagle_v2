<%if user_signed_in?%>
<%=form_with model:@service_center_admin,url: service_center_admin_path,local: true do |f|%>
    
    <p>
        <%=f.label :firstname%>
        <%=f.text_field :firstname%>
    </p>
        
    
    <p>
        <%=f.label :laststname%>
        <%=f.text_field :lastname%>
    </p>

    
    <p>
        <%=f.label :username%>
        <%=f.text_field :username%>
    </p>

    
    <p>
        <%=f.label :email%>
        <%=f.text_field :email%>
    </p>

    
    <p>
        <%=f.label :contact%>
        <%=f.text_field :contact%>
    </p>

    <p>
        <%=f.label :password%>
        <%=f.password_field :password%>
    </p>
    
    <p>
        <%=f.label :password_confirmation%>
        <%=f.password_field :password_confirmation%>
    </p>
    <%=f.submit "update admin"%>
<%end%>
<%end%>