<%if user_signed_in?%>
<div class= "container">
<div class= "row">
<div class= "col-md-6">
<%=form_with model:@service_center_admin,url: service_center_admin_path,local: true do |f|%>
    
    <div class= "form-group">
        <%=f.label :firstname%><br/>
        <%=f.text_field :firstname%>
    </div>
        
    
    <div class= "form-group">
        <%=f.label :laststname%><br/>
        <%=f.text_field :lastname%>
    </div>

    
    <div class= "form-group">
        <%=f.label :username%>
        <%=f.text_field :username%>
    </div>

    
    <div class= "form-group">
        <%=f.label :email%>
        <%=f.text_field :email%>
    </div>

    
    <div class= "form-group">
        <%=f.label :contact%>
        <%=f.text_field :contact%>
    </div>

    <div class= "form-group">
        <%=f.label :password%>
        <%=f.password_field :password%>
    </div>
    
    <div class= "form-group">
        <%=f.label :password_confirmation%>
        <%=f.password_field :password_confirmation%>
    </div>
    <%=f.submit "update admin"%>
<%end%>
<%else%>
<h3>Please login!!!!!!!!</h3>
<h3>If you are new user then sign up</h3>
<%= link_to 'sign in', new_user_registration_path %>
<%= link_to 'sign in', new_user_session_path %>
<%end%>
<%end%>